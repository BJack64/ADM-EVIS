    function PreviewFileECM(objectID) {
        var url = "/ExportDownload/PreviewFileEcm?objectID=" + objectID;

        $.ajax({
            url: url,
            type: 'get',
        }).done(function (data, statusText, xhr) {
            var status = xhr.status; //200
            if (status == 200) {
                window.open(url, '_blank').focus();
            } else {
                toastr.warning(xhr.responseText, "Unable to preview file", { "positionClass": "toast-top-center", "showDuration": "30000" });
            }
        });
    }

