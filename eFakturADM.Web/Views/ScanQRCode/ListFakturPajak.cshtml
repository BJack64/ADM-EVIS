<script>
    document.addEventListener("DOMContentLoaded", function () {
        // Checkbox untuk memilih semua checkbox lainnya
        const selectAllCheckbox = document.getElementById("selectall");

        // Event untuk mengatur semua checkbox sebagai tercentang atau tidak saat checkbox "select all" diklik
        selectAllCheckbox.addEventListener("change", function () {
            const checkboxes = document.querySelectorAll("#tbl-fakturpajak tbody input[type='checkbox']");
            checkboxes.forEach((checkbox) => {
                checkbox.checked = selectAllCheckbox.checked;
            });
        });

        // Fungsi untuk mendapatkan nilai checkbox yang dipilih
        function getSelectedCheckboxes() {
            const selected = [];
            const checkboxes = document.querySelectorAll("#tbl-fakturpajak tbody input[type='checkbox']:checked");
            checkboxes.forEach((checkbox) => {
                selected.push(checkbox.value); // Tambahkan nilai checkbox yang dipilih
            });
            return selected;
        }

        // Contoh penggunaan fungsi di atas saat tombol diklik
        document.getElementById("getSelectedValuesButton").addEventListener("click", function () {
            const selectedValues = getSelectedCheckboxes();
            console.log("Selected Values:", selectedValues);
            // Lakukan aksi lain yang diinginkan dengan selectedValues
        });
    });


