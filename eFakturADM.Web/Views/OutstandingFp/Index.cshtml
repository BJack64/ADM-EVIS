    function deleteBatch() {
        dataToDelete = [];   
        if ($(".checkbox:checked").length == 0) {
            toastr.warning("No Data Selected", "", {
                "positionClass": "toast-top-center",
                "showDuration": "30000"
            });
        } else {
            $('#tbl-fakturpajak').find('tr').each(function () {
                var row = $(this);
                if (row.find('input[type="checkbox"]').is(':checked')) {
                    var fpId = $(this).find('.checkbox').val();
                    if (fpId != undefined) {
                        dataToDelete.push(parseInt(fpId));
                    }
                }
            });


            if (dataToDelete.length > 0) {
                    var selectAll = $('#selectall').is(':checked');

                NoFaktur1 = $("#input-search-nofaktur1").val();
                NoFaktur2 = $("#input-search-nofaktur2").val();
                NPWPVendor = $("#input-search-npwpvendor").val();
                NamaVendor = $("#input-search-namavendor").val();
                TglFakturStart = $("#input-search-tglawal").val();
                TglFakturEnd = $("#input-search-tglakhir").val();
                Status = $("#lst-status-approval option:selected").val();
                ScanDateAwal = $('#input-search-scandate-awal').val();
                ScanDateAkhir = $('#input-search-scandate-akhir').val();
                Source = $('#lst-source option:selected').val();
                StatusFaktur = $('#lst-statusfaktur option:selected').text();
                Remark = $('#input-search-remark').val();
                ByPass = $('#lst-bypass option:selected').val();
                    sSearch_1 = $('#input-ssearch1').val();
                    sSearch_2 = $('#input-ssearch2').val();
                    sSearch_3 = $('#input-ssearch3').val();
                    sSearch_4 = $('#input-ssearch4').val();
                    sSearch_5 = $('#input-ssearch5').val();
                    sSearch_6 = $('#input-ssearch6').val();
                    sSearch_7 = $('#input-ssearch7').val();
                    sSearch_8 = $('#input-ssearch8').val();
                    sSearch_9 = $('#input-ssearch9').val();
					sSearch_10 = $('#input-ssearch10').val();
					sSearch_11 = $('#input-ssearch11').val();
					sSearch_12 = $('#input-ssearch12').val();
                var oData = JSON.stringify({
                    FakturPajakIds: dataToDelete,
                    NoFaktur1: NoFaktur1,
                    NoFaktur2: NoFaktur2,
                    NPWP: NPWPVendor,
                    Nama: NamaVendor,
                    TglFakturStart: TglFakturStart,
                    TglFakturEnd: TglFakturEnd,
                    Status: Status,
                    scanDateAwal: ScanDateAwal,
                    scanDateAkhir: ScanDateAkhir,
                    receivedStart: ReceivedStart,
                    receivedEnd: ReceivedEnd,
                    source: Source,
                    statusFaktur: StatusFaktur,
                    remark: Remark,
                    isByPass: ByPass,
                    sSearch_1: sSearch_1,
                    sSearch_2: sSearch_2,
                    sSearch_3: sSearch_3,
                    sSearch_4: sSearch_4,
                    sSearch_5: sSearch_5,
                    sSearch_6: sSearch_6,
                    sSearch_7: sSearch_7,
                    sSearch_8: sSearch_8,
                    sSearch_9: sSearch_9,
                    sSearch_10: sSearch_10,
                    sSearch_11: sSearch_11,
                    sSearch_12: sSearch_12,
                    selectAll: selectAll
                });

                var txt = $("#tbl-fakturpajak_info").text();
                var s = txt.indexOf('of');
                var f = txt.indexOf('entries');
                var totalRecords = Number(txt.substr(s + 2, f - (s + 2)));
                
                BootstrapDialog.show({
                    type: BootstrapDialog.TYPE_DANGER,
                    title: 'Delete Confirmation',
                    //message: '[' + $(".checkbox:checked").length + ']' + '  Faktur Pajak Akan di Hapus' + '<br/>' + 'dan akan dipindahkan ke Menu Penampung',
                    message: 'Faktur Pajak Akan di Hapus' + '<br/>' + 'dan akan dipindahkan ke Menu Penampung : ' + (selectAll ? totalRecords:$(".checkbox:checked").length),
                    buttons: [{
                        label: 'Delete',
                        cssClass: 'btn-danger',
                        action: function (dialog) {
                            $.ajax({
                                type: "POST",
                                url: '@Url.Action("DeleteFakturPajakOutstanding", "OutstandingFp")',
                                dataType: "json",
                                contentType: 'application/json; charset=utf-8',
                                data: oData,
                                cache: false,
                                success: function (data) {
                                    switch (data.Html.InfoType) {
                                        case 0:
                                            toastr.info(data.Html.Message, "", {
                                                "positionClass": "toast-top-center",
                                                "showDuration": "30000"
                                            });
                                            dialog.close();
                                            break;
                                        case 1:
                                            toastr.success(data.Html.Message, "", {
                                                "positionClass": "toast-top-center",
                                                "showDuration": "30000"
                                            });
                                            dialog.close();
                                            break;
                                        case 2:
                                            toastr.warning(data.Html.Message, "", {
                                                "positionClass": "toast-top-center",
                                                "showDuration": "30000"
                                            });
                                            break;
                                        case 3:
                                            toastr.error(data.Html.Message, "", {
                                                "positionClass": "toast-top-center",
                                                "showDuration": "30000"
                                            });
                                            dialog.close();
                                            break;
                                    }
                                    var table = $('#tbl-fakturpajak').DataTable();
                                    table.ajax.reload();
                                },
                                error: function (xhr, textStatus, errorThrown) {
                                    App.DialogError(xhr, textStatus, errorThrown);
                                }
                            });
                        }
                    }, {
                        label: 'Cancel',
                        action: function (dialog) {
                            dialog.close();
                        }
                    }]
                });
            }

        }
    }

